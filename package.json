{
  "name": "tailwind-prefix-highlighter",
  "displayName": "Tailwind Prefix Highlighter",
  "description": "Highlights Tailwind CSS responsive prefixes and state modifiers",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:svelte"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Tailwind Prefix Highlighter",
      "properties": {
        "tailwindPrefixHighlighter.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable the Tailwind prefix highlighter"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@vscode/vsce": "^3.6.0"
  }
}
