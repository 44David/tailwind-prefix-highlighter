{
  "name": "tailwind-prefix-highlighter",
  "displayName": "Tailwind Prefix Highlighter",
  "description": "Highlights Tailwind CSS responsive prefixes and state modifiers",
  "version": "1.0.0",
  "publisher": "44David",
  "keywords": [
    "tailwind",
    "css",
    "highlight",
    "responsive",
    "prefixes"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:svelte"
  ],
  "main": "./out/extension.js",
  "files": [
    "out/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "contributes": {
    "configuration": {
      "title": "Tailwind Prefix Highlighter",
      "properties": {
        "tailwindPrefixHighlighter.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable the Tailwind prefix highlighter"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
